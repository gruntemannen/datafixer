# Frontend environment variables (for local development)
# Copy this file to .env and fill in the values after deployment

# API Gateway endpoint (from CDK output)
VITE_API_ENDPOINT=https://xxxxxxxxxx.execute-api.eu-central-1.amazonaws.com

# Cognito User Pool ID (from CDK output)
VITE_USER_POOL_ID=eu-central-1_xxxxxxxxx

# Cognito User Pool Client ID (from CDK output)
VITE_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# AWS Region
VITE_REGION=eu-central-1
