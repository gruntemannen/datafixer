{
  "name": "datafixer-infra",
  "version": "1.0.0",
  "description": "CDK Infrastructure for DataFixer",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all --require-approval never",
    "deploy:subaccount": "cdk deploy --all --require-approval never --profile datafixer-deploy",
    "destroy": "cdk destroy --all --force",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.120.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.3.0"
  }
}
